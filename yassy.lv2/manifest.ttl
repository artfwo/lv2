@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix kx:    <http://kxstudio.sf.net/ns/lv2ext/external-ui#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .

# Credits to Hanspeter Portner for explaining how ui:UI and kx:Widget work. See
# http://lists.lv2plug.in/pipermail/devel-lv2plug.in/2016-May/001649.html
# to please sord_validate
kx:Widget
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf ui:UI .
kx:Host
	a lv2:Feature .
	
<http://example.org/yassy>
	a lv2:Plugin ;
	lv2:binary <libyassy.so> ;
	ui:ui <http://example.org/yassy#ui> ;
	ui:ui <http://example.org/yassy#kx> ;
	rdfs:seeAlso <yassy.ttl> .
	
<http://example.org/yassy#ui>
	a ui:UI ;
	ui:binary <libyassyui.so> ;	
	rdfs:seeAlso <yassyui.ttl> .
	
<http://example.org/yassy#kx>	
	a kx:Widget ;
	ui:binary <libyassyui.so> ;	
	rdfs:seeAlso <yassyui.ttl> .
	